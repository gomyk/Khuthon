<!-- Room -->
<div class="tab-pane" id="{{room}}">
  <button class="btn btn" type="button" data-toggle="collapse" data-target="#collapseExample{{room}}" aria-expanded="false" aria-controls="collapseExample{{room}}" style="border:2px;margin-bottom:30px;margin-top:10px" >
      공지사항
    </button>
    <div class="collapse" id="collapseExample{{room}}">
        <div class="well">
            공지사항입니다.
        </div>
      </div>
    <div class="row-fluid">
        <div class="span12 well">
            <div id="room_messages" style="min-height:220px; max-height:220px; overflow:auto;">
                <span class="label label-important">Admin</span>&nbsp;&nbsp;반가워용~~:)<br/>
            </div>
        </div>
    </div>
    <div class="row-fluid">
        <div class="span12">
            <h3>Current chat User List</h3>
        </div>
    </div>
    <div class="row-fluid">
        <div id="room_users" class="span12">
            <span class="badge badge-important">Admin</span>
        </div>
    </div>
</div>


<!-- End Room -->
